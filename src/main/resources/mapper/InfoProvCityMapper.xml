<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.monetware.ringsurvey.business.dao.InfoProvinceCityDao">

    <!-- 获取镇列表 -->
    <select id="getTownNameList" resultType="java.lang.String"
            parameterType="com.monetware.ringsurvey.business.pojo.vo.sample.TownVO">
        SELECT distinct
        town_name
        FROM rs_info_prov_city
        WHERE province_name = #{provinceName}
        AND city_name = #{cityName}
        AND district_name = #{distName}
    </select>



</mapper>